{
  "name": "@emartech/fake-server",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha test-setup.spec.js lib --recursive --exit",
    "semantic-release": "CI=true semantic-release pre && npm publish --access public && semantic-release post"
  },
  "config": {
    "commitTypeMap": {
      "test": "patch",
      "chore": "patch",
      "refactor": "patch"
    }
  },
  "release": {
    "analyzeCommits": "@bubltechnology/customizable-commit-analyzer",
    "verifyConditions": {
      "path": "./node_modules/semantic-release/src/lib/plugin-noop.js"
    }
  },
  "engines": {
    "node": ">= 8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emartech/fake-server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/emartech/fake-server/issues"
  },
  "homepage": "https://github.com/emartech/fake-server#readme",
  "dependencies": {
    "@koa/cors": "2.2.1",
    "jsonwebtoken": "7.4.1",
    "koa": "2.4.1",
    "koa-bodyparser": "4.2.0",
    "koa-escher-auth": "2.5.4",
    "koa-jwt": "3.3.0",
    "koa-router": "7.3.0",
    "lodash": "4.17.4"
  },
  "devDependencies": {
    "@bubltechnology/customizable-commit-analyzer": "1.0.2-0",
    "semantic-release": "7.0.2",
    "chai": "4.1.2",
    "mocha": "4.0.1",
    "sinon": "4.1.2",
    "sinon-chai": "2.14.0",
    "supertest": "3.0.0"
  }
}
